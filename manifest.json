{
  "name": "LeetCode - Profile View Blocker",
  "description": "Blocks you from viewing profiles on LeetCode including you own.",
  "version": "1.1.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["https://leetcode.com/*"],
      "js": ["Content.js"]
    }
  ],
  "permissions": ["webNavigation"],
  "icons": {
    "16": "icons/eye-crossed-16.png",
    "128": "icons/eye-crossed-128.png"
  }
}
